generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id    String @id @default(uuid())
  name  String
  email String @unique
  score Int    @default(0)

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}